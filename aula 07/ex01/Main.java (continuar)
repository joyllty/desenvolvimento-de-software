import java.util.Scanner;
//ler sobre construtores, static, encapsulamento
public class Main {

    public static void main(String[] args) {

        //construtor?
        Funcionario[] funcios = new Funcionario[5];

        //inicializando o vetor de funcionarios
        for (int i = 0; i < funcios.length; i++) {
            funcios[i] = new Funcionario();
        }


    }
}
